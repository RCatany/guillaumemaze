<?xml version="1.0" encoding="UTF-8"?>
<Module>
  <ModulePrefs title="Read my PhD" description="Read my PhD manuscript online with Scribd" author="Guillaume Maze" author_email="code@guillaumemaze.org" author_link="http://www.guillaumemaze.org" width="580" height="860" scrolling="false">
	<Require feature="flash" />
</ModulePrefs>
<Content type="html">
	<![CDATA[

	
	<script type="text/javascript" src='http://www.scribd.com/javascripts/view.js'></script> 
	<!-- The contents of this div will get replaced with the iPaper -->
	<div id='embedded_flash' > 
		My PhD Manuscript should appear here ...
	</div>
	<script type="text/javascript">
		var scribd_doc = scribd.Document.getDoc(2077402, 'key-av7lrk3vkm8wwnmq8nt'); 
		scribd_doc.addParam('height', 800);
	  scribd_doc.addParam('width', 570);
	  scribd_doc.addParam('page', 1);
	  scribd_doc.addParam('mode', 'list');
	  scribd_doc.addParam('public', true);
	  
		scribd_doc.write('embedded_flash');  
	</script>




	]]>
</Content>
</Module>
